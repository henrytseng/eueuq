{
  "name": "eueuq",
  "version": "0.0.2",
  "private": true,
  "description": "A flexible messaging queue with priority handling",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/henrytseng/eueuq.git"
  },
  "author": "Henry Tseng <info@heycanvas.com> (http://www.heycanvas.com/)",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start": "DEBUG=eueuq* packages/eueuq-broker/bin/eueuq",
    "test": "lerna run test",
    "lint": "node_modules/.bin/eslint packages/*/lib/**/*.js packages/*/test/**/*.js",
    "watch": "DEBUG=eueuq* node_modules/.bin/jest packages/*/test --detectOpenHandles --watch"
  },
  "keywords": [
    "worker",
    "consumer",
    "scheduler",
    "producer",
    "job",
    "priority",
    "messaging",
    "queue"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henrytseng/eueuq/issues"
  },
  "homepage": "https://github.com/henrytseng/eueuq#readme",
  "devDependencies": {
    "jest": "^23.1.0",
    "lerna": "^2.11.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2",
    "yarn": "^1.7.0"
  },
  "engines": {
    "node": ">= 6.x <= 9.x",
    "npm": ">= 2.x <= 5.x"
  },
  "workspaces": [
    "packages/*"
  ]
}
